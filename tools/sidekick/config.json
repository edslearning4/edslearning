{
  "project": "EDS + Document Based Authoring",
  "plugins": [
    {
      "id": "asset-library",
      "title": "AEM Assets",
      "environments": ["edit"],
      "url": "https://experience.adobe.com/solutions/CQ-helix-assets-addon/static-assets/resources/asset-selector.html?rail=false",
      "isPalette": true,
      "includePaths": ["**.docx**"],
      "passConfig": true,
      "passReferrer": true,
      "paletteRect": "top:50px;right:10px;width:600px;height:calc(100vh - 60px)"
    },
    {
      "id": "google-assist",
      "title": "Google Assist",
      "environments": ["any"],
      "url": "/tools/google-assist/index.html",
      "isPalette": true,
      "paletteRect": "top:50px;right:10px;width:400px;height:calc(100vh - 60px)"
    },
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true
    },
    {
      "containerId": "tools",
      "id": "localize",
      "title": "Localize",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "https://www.microsoft.com/en-us/translator/business/translator-api/",
      "passReferrer": true
    },
    {
      "containerId": "tools",
      "id": "youtube",
      "title": "YouTube",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "https://www.youtube.com",
      "passReferrer": true
    }
  ]
}
